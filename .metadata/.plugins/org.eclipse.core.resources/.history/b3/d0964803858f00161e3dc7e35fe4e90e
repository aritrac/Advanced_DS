import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.*;

public class Solutions {

	public static void main(String[] args) throws IOException{
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		String st[] = br.readLine().split(" ");
		int N = Integer.valueOf(st[0]);
		int Q = Integer.valueOf(st[1]);
		if (N < 1 || N > 100000)
			return;
		if (Q < 1 || Q > 100000)
			return;
		int lastAns = 0;
		List<List<Integer>> seq = new ArrayList<List<Integer>>();
		for (int j = 0; j < N; j++) {
			seq.add(new ArrayList<Integer>());
		}

		for (int i = 0; i < Q ; i++) {
			String str[] = br.readLine().split(" ");
			int type = Integer.valueOf(str[0]);
			int x = Integer.valueOf(str[1]);
			int y = Integer.valueOf(str[2]);			
			if (type == 1) {
				int s = (x ^ lastAns) % N;
				seq.get(s).add(y);
			}
			if (type == 2) {
				int s = (x ^ lastAns) % N;
				lastAns = seq.get(s).get(y % (seq.get(s).size()));
				System.out.println(lastAns);
			}
		}

	}
}