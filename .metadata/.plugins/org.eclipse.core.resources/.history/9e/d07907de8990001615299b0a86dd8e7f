import java.util.*;

class Solutions {
    public static void main(String args[] ) throws Exception {
        Scanner in = new Scanner(System.in);
        long N = in.nextLong();
        String str = in.nextLine();
        //hackerearth
        int h_count = 2;
        int a_count = 2;
        int c_count = 1;
        int k_count = 1;
        int e_count = 2;
        int r_count = 2;
        int t_count = 1;
        
        int[] char_count = new int[7];
        for(int i = 0; i < N; i++){
        	if(str.charAt(i) == 'h')
        		char_count[0]++;
        	if(str.charAt(i) == 'a')
        		char_count[1]++;
        	if(str.charAt(i) == 'c')
        		char_count[2]++;
        	if(str.charAt(i) == 'k')
        		char_count[3]++;
        	if(str.charAt(i) == 'e')
        		char_count[4]++;
        	if(str.charAt(i) == 'r')
        		char_count[5]++;
        	if(str.charAt(i) == 't')
        		char_count[6]++;
        }
        
        for(int i = 0; i < 7; i++){
        	if(char_count[i] == 0){
        		System.out.println(0);
        		return;
        	}
        }
        int times = 0;
        
        while(true){
        	if(char_count[0] - 2 >= 0 && char_count[1] - 2 >= 0 && char_count[2] - 1 >= 0 && char_count[3] - 1 >= 0 && char_count[4] - 2 >= 0 && char_count[5] - 2 >= 0 && char_count[6]-1 >= 0){
        		times++;
        		char_count[0] -= 2;
        		char_count[1] -= 2;
        		char_count[2] -= 1;
        		char_count[3] -= 1;
        		char_count[4] -= 2;
        		char_count[5] -= 2;
        		char_count[6] -= 1;
        	}else{
        		break;
        	}
        }
        System.out.println(times);
    }
}
